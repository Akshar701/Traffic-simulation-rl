<?xml version="1.0" encoding="UTF-8"?>
<connections>
    <!-- North approach connections -->
    <connection from="N_in" fromLane="0" to="C_E" toLane="0" />
    <connection from="N_in" fromLane="1" to="C_S" toLane="1" />
    <connection from="N_in" fromLane="2" to="C_S" toLane="2" />
    <connection from="N_in" fromLane="3" to="C_W" toLane="3" />
    
    <!-- South approach connections -->
    <connection from="S_in" fromLane="0" to="C_W" toLane="0" />
    <connection from="S_in" fromLane="1" to="C_N" toLane="1" />
    <connection from="S_in" fromLane="2" to="C_N" toLane="2" />
    <connection from="S_in" fromLane="3" to="C_E" toLane="3" />
    
    <!-- East approach connections -->
    <connection from="E_in" fromLane="0" to="C_S" toLane="0" />
    <connection from="E_in" fromLane="1" to="C_W" toLane="1" />
    <connection from="E_in" fromLane="2" to="C_W" toLane="2" />
    <connection from="E_in" fromLane="3" to="C_N" toLane="3" />
    
    <!-- West approach connections -->
    <connection from="W_in" fromLane="0" to="C_N" toLane="0" />
    <connection from="W_in" fromLane="1" to="C_E" toLane="1" />
    <connection from="W_in" fromLane="2" to="C_E" toLane="2" />
    <connection from="W_in" fromLane="3" to="C_S" toLane="3" />
</connections>